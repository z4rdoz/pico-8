pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- global variables
t=0
last_bullet=3

---------------------

-- pseudo-classes
function new_player()
	local p = {
		health = 100
	}
	return p
end

function new_enemy(x,y)
	local e ={
		sp=17,
		init_x=x,
		init_y=y,
		x=x,
		y=y,
		r=10,
	 update=function(self)
			self.x=self.init_x+
				(sin(t/50)*self.r)
			self.y=self.init_y+
				(cos(t/50)*self.r)
	 end	 
 }
 return e
end

function new_ship(x, y)
	local s = {
		sp=1,
		x=x,
		y=y,
		update=function(self)
			if(t%6<3) then
				ship.sp=1
			else
				ship.sp=2
			end
		end
		}
	return s
end

function new_bullet()
	local b = {
		sp=3,
		x=ship.x,
		y=ship.y,
		dx=0,
		dy=-3,	
		update=function(self)
			self.x += self.dx
			self.y += self.dy
			if is_offscreen(self.x,self.y) then
				del(bullets,self)
			end
		end	
	}
	if last_bullet == 3 then
		b.sp=4
	end
	last_bullet = b.sp
	return b
end
--------------------

-- core functions
function fire()
	b = new_bullet()
	add(bullets,b)
end

--------------------

-- helper functions
function is_offscreen(x,y)
	if x < 0 or x > 128
		or y < 0 or y > 128 then
		return true
	end
	return false
end

function ceil(x)
	return -flr(-x)
end
--------------------

-- system functions
function _init()
	ship = new_ship(60,60)
	player = new_player()
	bullets = {}
	enemies = {}
	for i=1,10 do		
		add(enemies, 
			new_enemy(i*16,30-i*8))
	end
end

function _update()
	t+=1
	
	ship:update()
	
	for b in all(bullets) do
		b:update()
	end
	
	for e in all(enemies) do
		e:update()
	end
	
	if btn(0) then ship.x-=1 end
	if btn(1) then ship.x+=1 end
	if btn(2) then ship.y-=1 end
	if btn(3) then ship.y+=1 end
	if btnp(4) then fire() end
end

function _draw()
	cls()
	palt()
	
	spr(ship.sp,ship.x,ship.y)
	
	for b in all(bullets) do
		spr(b.sp,b.x,b.y)
	end
	
	for e in all(enemies) do
		spr(e.sp,e.x,e.y)
	end
	
	--health
 palt(11,true)
 palt(0,false)
	
	spr(5,5,5)
	spr(6,13,5)
	spr(7,21,5)
	local h = player.health*22/100	
  
 
	for i=1,h do
		line(i+5,6,i+5,7, 8)
	end 
end
